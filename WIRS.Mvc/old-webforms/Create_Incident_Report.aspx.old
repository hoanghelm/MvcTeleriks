     function SearchInjuredPerson()
     {
         var txtInjured_Name;

         if (document.getElementById('ctl00_Content_txtInjured_Name').value == null) {
             txtInjured_Name = "";
         }
         else {
             txtInjured_Name = document.getElementById('ctl00_Content_txtInjured_Name').value;
         }

         var popup =
         window.open("EmployeeSearch.aspx?SearchType=InjuredSearch&Name="
                     + txtInjured_Name + "&SBU=" + "&IC="
                     , "_blank", "toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,top=200,left=300,width=800,height=400");
         popup.focus();
     }

     function setInjuredSearch(id, name, contactno, nric, age, race, nationlity, gender, employmenttype, dateofemployment, empdesignation, empemailaddress) {
         document.getElementById('ctl00_Content_txtInjured_EmployeeNo').value = id;
         document.getElementById('ctl00_Content_txtInjured_Name').value = name;
         document.getElementById('ctl00_Content_txtInjured_ContactNo').value = contactno;
         document.getElementById('ctl00_Content_hdnInjured_Age').value = age;
         document.getElementById('ctl00_Content_txtInjured_Race').value = race;
         document.getElementById('ctl00_Content_txtInjured_Nationlity').value = nationlity;
         document.getElementById('ctl00_Content_txtInjured_Gender').value = gender;
         document.getElementById('ctl00_Content_txtInjured_EmploymentType').value = employmenttype;
         document.getElementById('ctl00_Content_txtInjured_DateofEmployment').value = dateofemployment;
         document.getElementById('ctl00_Content_txtInjured_empdesignation').value = empdesignation;
     }
